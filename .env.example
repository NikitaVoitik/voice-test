# Deepgram API Key (for realtime transcription)
# Get your API key from https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Deepgram Project ID (optional, for temporary key generation)
# DEEPGRAM_PROJECT_ID=your_project_id_here

# ElevenLabs API Key
# Get your API key from https://elevenlabs.io/
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ElevenLabs Voice ID (optional, defaults to Rachel voice)
# ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# ElevenLabs Model ID (optional, defaults to eleven_monolingual_v1)
# ELEVENLABS_MODEL_ID=eleven_monolingual_v1

# Cartesia API Key
# Get your API key from https://cartesia.ai/
CARTESIA_API_KEY=your_cartesia_api_key_here

# Cartesia Voice ID (optional, defaults to Barbershop Man)
# CARTESIA_VOICE_ID=a0e99841-438c-4a64-b679-ae501e7d6091

# Cartesia Model ID (optional, defaults to sonic-english)
# CARTESIA_MODEL_ID=sonic-english

# OpenAI API Key
# Get your API key from https://platform.openai.com/
OPENAI_API_KEY=your_openai_api_key_here
